    "eslint-plugin-react-hooks": "^4.2.0",
    "fuse.js": "^3.4.5",
    "jest": "^26.6.3",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "^17.0.1",
    "react-transition-group": "^4.4.1"
  },
