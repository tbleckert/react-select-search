{
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}
